extends layout

block content
  .page-header
    h3 Projects

  .row
    // Team member
    .col-xs-12.col-sm-6.col-md-4
      .image-flip(ontouchstart="this.classList.toggle('hover');")
        .mainflip
          .frontside
            .card
              .card-body.text-center
                p
                  img.img-fluid(src='https://sunlimetech.com/portfolio/boot4menu/assets/imgs/team/img_01.png', alt='card image')
                h4.card-title Sunlimetech
                p.card-text This is basic card with image on top, title, description and button.
                a.btn.btn-primary.btn-sm(href='#')
                  i.fa.fa-plus
          .backside
            .card
              .card-body.text-center.mt-4
                h4.card-title Sunlimetech
                p.card-text
                  | This is basic card with image on top, title, description and button.This is basic card with image on top, title, description and button.This is basic card with image on top, title, description and button.
                ul.list-inline
                  li.list-inline-item
                    a.social-icon.text-xs-center(target='_blank', href='#')
                      i.fa.fa-facebook
                  li.list-inline-item
                    a.social-icon.text-xs-center(target='_blank', href='#')
                      i.fa.fa-twitter
                  li.list-inline-item
                    a.social-icon.text-xs-center(target='_blank', href='#')
                      i.fa.fa-skype
                  li.list-inline-item
                    a.social-icon.text-xs-center(target='_blank', href='#')
                      i.fa.fa-google
